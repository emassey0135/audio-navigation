{
	"schemaVersion": 1,
	"id": "audio-navigation",
	"version": "${version}",
	"name": "Audio Navigation",
	"description": "A mod that announces points of interest around the player, inspired by Soundscape",
	"authors": [
		"Elijah Massey"
	],
	"contact": {
		"homepage": "https://github.com/emassey0135/audio-navigation",
		"sources": "https://github.com/emassey0135/audio-navigation"
	},
	"license": "GPL-3.0",
	"icon": "assets/audio-navigation/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "dev.emassey0135.audionavigation.AudioNavigation",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "dev.emassey0135.audionavigation.AudioNavigationClient",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"audio-navigation.mixins.json",
		{
			"config": "audio-navigation.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": "~1.21.4",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"minecraft-access": "*"
	}
}